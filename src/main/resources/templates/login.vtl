<form action="/users/login" method="post">
    <select name="user_id">
        #foreach($user in $users)
            <option value="$user.getId()">$user.getName()</option>
        #end
    </select>

   <input type="submit" value="LOG IN" class="button center">
</form>

<a href="/users/new">Create User Account</a>

<a class="admin-login" href="/admin">Login as Admin</a>


