<a class="new" href="/admin/symbol-categories/new">Add category</a>
<h1 class="pageHeader">All Categories</h1>

    <table class="table">
        #foreach($symbolCategory in $symbolCategories)
        <tr class="padding-left">
            <td class="padding-left"><i class="$symbolCategory.getIcon()"></i></td>
            <td class="padding-left">$symbolCategory.getDescriptor()</td>
            <td class="td-actions">
                <a href="/admin/symbol-categories/$symbolCategory.getId()/edit"><i class="edit fas fa-edit"></i></a>
                <form class="delete-form" action="/admin/symbol-categories/$symbolCategory.getId()/delete" method="post">
                    <button><i class="delete fas fa-trash-alt"></i></button>
                </form>
            </td>
        <tr>
            #end
    </table>

