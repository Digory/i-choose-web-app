<a class="new" href="/admin/timetables/new">Add Timetable</a>
<h1 class="pageHeader">All Timetables</h1>

    <table class="table">
        #foreach($timetable in $timetables)

        <tr>
            <td><a href="/admin/timetables/$timetable.getId()">$timetable.getName()</a></td>
            <td>$timetable.getUser().getName()</td>
            <td><a href="/admin/timetables/$timetable.getId()/show_symbols">SHOW SYMBOLS</a></td>
        </tr>

        #end

    </table>



