<h1>Add to timetable</h1>

<div class="categories">
    <ul>
        #foreach($timetable in $userTimetables)
        <li>
            <form action="/symbols/$symbol.getId()/add?user_id=$user.getId()&timetable_id=$timetable.getId()&searchQuery=$searchQuery" method="post">
                <input type="submit" value="$timetable.getName()" class="center">
            </form>
        </li>add_symbol
        #end
    </ul>
</div>