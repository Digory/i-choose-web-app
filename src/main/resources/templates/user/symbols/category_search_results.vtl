<h1 class="pageHeader">$categoryName:</h1>

    <table class="table">

        #if( $results.size() == 0 )
            <p class="margin-left">There are no symbols for this category â˜¹</p></td>

        #else

            #foreach($symbol in $results)
            <tr>
                <td class="td-image"><img src="$symbol.getImageUrl()"/></td>
