<h1 class="pageHeader">Search results:</h1>

    <table class="table">

        #if( $results.size() == 0 )
        <p class="margin-left">There are no symbols matching â˜¹</p>

        #else

        #foreach($symbol in $results)
        <tr>
            <td class="td-image"><a href="/users/$user.getId()/view_symbol?symbol_id=$symbol.getId()"><img src="$symbol.getImageUrl()"/></a></td>
            <td>$symbol.getCategory().getDescriptor():<br> <strong>$symbol.getName()</strong></td>
            <td>

                <a href="/users/$user.getId()/add_symbol?symbol_id=$symbol.getId()&searchQuery=$searchQuery">ADD TO TIMETABLE</a>
            </td>
        <tr>
            #end
        #end
    </table>
</div>

<a href="/users/$user.getId()">Search again</a>

